{
  "name": "fincado",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "clean": "rimraf out .next",
    "prebuild": "node src/scripts/process-wiki.js",
    "postbuild": "npm run check-links:ci",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "ping": "node src/scripts/ping-indexnow.js",
    "fix-links": "node src/scripts/fix-all-broken-links.js && node src/scripts/search-and-replace.js && node src/scripts/create-emi-redirects.js && node src/scripts/create-placeholder-images.js",
    "check-links": "echo 'Link checking disabled - use after deployment'",
    "check-internal": "tsx src/scripts/check-internal-links.ts",
    "link-dashboard": "tsx src/scripts/link-health-dashboard.ts",
    "check-links:local": "echo 'Link checking disabled - use after deployment'",
    "check-links:ci": "echo 'Skipping link check in CI - run manually after deployment'",
    "scan:deep": "node src/scripts/deep-link-scan.js",
    "scan:local": "node src/scripts/deep-link-scan.js http://localhost:3000",
    "scan:prod": "node src/scripts/deep-link-scan.js https://fincado.com",
    "audit:calculators": "node src/scripts/calculator-checker.js"
  },
  "dependencies": {
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-dialog": "^1.1.15",
